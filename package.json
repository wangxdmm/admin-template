{
  "name": "@runafe/easy-admin",
  "type": "module",
  "version": "0.0.12",
  "description": "Admin based on naive-ui vite UnoCSS and Typescript",
  "keywords": [
    "admin",
    "naive-ui",
    "UnoCSS"
  ],
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/es/index.js"
    },
    "./theme/vars": {
      "types": "./dist/es/theme/vars.d.ts",
      "import": "./dist/es/theme/vars.js",
      "require": "./dist/cjs/theme/vars.cjs",
      "default": "./dist/es/theme/vars.cjs"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./*": "./*"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist",
    "style"
  ],
  "scripts": {
    "clear": "rimraf ./temp",
    "build": "vite build && pnpm build:dts",
    "build:builder": "pnpm -C apps/builder build",
    "build:dts": "pnpm clear && pnpm exec vue-tsc -p tsconfig.dts.json && cp tsconfig.temp.json temp/tsconfig.json && resolve-tspaths -p temp/tsconfig.json && esno scripts/patch-dts.ts && pnpm clear",
    "release": "esno scripts/release.ts",
    "up:core": "pnpm up \"@runafe/*\" \"@monan/*\" \"@formkit/*\" \"vue\" --latest -r",
    "up:dev": "pnpm up \"@antfu/*\" \"@unocss/*\" \"unocss\"  --latest -r"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@iconify/vue": "^4.1.2",
    "@monan/shared": "^1.0.4",
    "@runafe/formkit-naive-ui": "^1.1.39",
    "@runafe/magic-system": "^1.3.72",
    "@vueuse/core": "^10.9.0",
    "clipboard": "^2.0.11",
    "colord": "^2.9.3",
    "dayjs": "^1.11.11",
    "hookable": "^5.5.3",
    "js-file-downloader": "^1.1.25",
    "localforage": "^1.10.0",
    "merge-anything": "^5.1.7",
    "naive-ui": "npm:@runafe/naive-ui@2.38.1-lamp.15",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinyin-pro": "^3.20.4",
    "qs": "^6.12.1",
    "simplebar-vue": "^2.3.4",
    "theme-colors": "^0.1.0",
    "universal-cookie": "^7.1.4",
    "vue": "3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@iconify/json": "2.2.209",
    "@runafe/elegant-router-vue": "1.0.11",
    "@runafe/tools-build": "^1.0.6",
    "@types/node": "20.12.11",
    "@types/nprogress": "0.2.3",
    "@types/qs": "^6.9.15",
    "@unocss/eslint-config": "0.60.2",
    "@unocss/preset-icons": "0.60.2",
    "@unocss/preset-uno": "0.60.2",
    "@unocss/transformer-compile-class": "^0.60.2",
    "@unocss/transformer-directives": "0.60.2",
    "@unocss/transformer-variant-group": "0.60.2",
    "@unocss/vite": "0.60.2",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "bumpp": "^9.4.1",
    "consola": "^3.2.3",
    "enquirer": "2.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-vue": "9.26.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "lint-staged": "^15.2.2",
    "move-file": "^3.1.0",
    "picocolors": "^1.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "resolve-tspaths": "^0.8.18",
    "rimraf": "5.0.7",
    "sass": "1.77.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "4.10.2",
    "typescript": "^5.4.5",
    "unocss": "^0.60.2",
    "unocss-tricks": "^1.0.4",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-icons": "0.19.0",
    "unplugin-vue-components": "0.27.0",
    "vite": "^5.2.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-inspect": "^0.8.4",
    "vite-plugin-no-bundle": "^4.0.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^1.6.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.17"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vue",
        "eslint",
        "naive-ui"
      ],
      "allowedVersions": {
        "unocss": "^0.55.1",
        "vue": "^3.3.4",
        "rollup": "^4.12.0",
        "eslint": "^9.0.0",
        "naive-ui": "*"
      }
    }
  },
  "simple-git-hooks": {},
  "lint-staged": {
    "*": "eslint --fix"
  },
  "volta": {
    "node": "20.11.1"
  }
}
