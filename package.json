{
  "name": "@runafe/easy-admin",
  "type": "module",
  "version": "0.0.1",
  "description": "Admin based on naive-ui vite UnoCSS and Typescript",
  "keywords": [
    "naive-ui",
    "UnoCSS"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.uno.js",
      "require": "./dist/index.uno.umd.cjs",
      "default": "./dist/index.uno.umd.cjs"
    },
    "./style": "./dist/style.css",
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./icons/*": "./icons/*",
    "./*": "./*"
  },
  "main": "./dist/index.uno.umd.cjs",
  "module": "./dist/index.uno.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "style"
  ],
  "scripts": {
    "dev": "pnpm -C builder dev",
    "clear": "rimraf ./temp",
    "dev:lib": "vite build --watch",
    "build": "vite build && pnpm build:dts",
    "build:dts": "pnpm clear && pnpm exec vue-tsc -p tsconfig.dts.json && cp tsconfig.temp.json temp/tsconfig.json && rollup -c rollup.dts.ts --configPlugin rollup-plugin-esbuild",
    "release": "esno scripts/release.ts"
  },
  "dependencies": {
    "@better-scroll/core": "2.5.1",
    "@iconify/vue": "4.1.1",
    "@monan/shared": "^1.0.2",
    "@runafe/formkit-naive-ui": "^1.1.26",
    "@runafe/magic-system": "1.3.23",
    "@vueuse/core": "10.9.0",
    "clipboard": "2.0.11",
    "colord": "2.9.3",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.10",
    "js-file-downloader": "^1.1.25",
    "localforage": "1.10.0",
    "merge-anything": "^5.1.7",
    "naive-ui": "npm:@runafe/naive-ui@2.38.1-lamp.15",
    "nanoid": "5.0.6",
    "nprogress": "0.2.0",
    "pinia": "2.1.7",
    "pinyin-pro": "^3.19.6",
    "qs": "^6.12.0",
    "rxjs": "^7.8.1",
    "simplebar-vue": "^2.3.3",
    "universal-cookie": "^7.1.4",
    "vue": "3.4.23",
    "vue-i18n": "9.10.2",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@iconify/json": "2.2.201",
    "@runafe/elegant-router-vue": "1.0.10",
    "@runafe/tools-build": "^1.0.6",
    "@types/crypto-js": "4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.29",
    "@types/nprogress": "0.2.3",
    "@types/qs": "^6.9.13",
    "@unocss/eslint-config": "0.59.0",
    "@unocss/preset-icons": "0.59.0",
    "@unocss/preset-uno": "0.59.0",
    "@unocss/transformer-compile-class": "^0.59.0",
    "@unocss/transformer-directives": "0.59.0",
    "@unocss/transformer-variant-group": "0.59.0",
    "@unocss/vite": "0.59.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "bumpp": "^9.4.0",
    "enquirer": "2.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "9.0.0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-plugin-vue": "9.23.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "picocolors": "^1.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "rimraf": "5.0.5",
    "rollup": "^4.13.0",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "sass": "1.72.0",
    "simple-git-hooks": "^2.11.0",
    "tsx": "4.7.1",
    "typescript": "^5.4.5",
    "unocss": "^0.59.0",
    "unocss-tricks": "^1.0.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "0.18.5",
    "unplugin-vue-components": "0.26.0",
    "vite": "5.1.6",
    "vite-bundle-visualizer": "^1.1.0",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-svg-icons": "2.0.1",
    "vitest": "^1.4.0",
    "vue-eslint-parser": "9.4.2",
    "vue-tsc": "2.0.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vue",
        "eslint",
        "naive-ui"
      ],
      "allowedVersions": {
        "unocss": "^0.55.1",
        "vue": "^3.3.4",
        "rollup": "^4.12.0",
        "eslint": "^9.0.0",
        "naive-ui": "*"
      }
    }
  },
  "simple-git-hooks": {},
  "lint-staged": {
    "*": "eslint --fix"
  },
  "volta": {
    "node": "20.11.1"
  }
}
